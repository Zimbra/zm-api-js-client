{
  "name": "@zimbra/api-client",
  "amdName": "zmApiJsClient",
  "version": "94.0.0",
  "description": "Zimbra JS API Client and GraphQL client for making requests against the Zimbra SOAP API.",
  "main": "dist/zm-api-js-client.js",
  "source": "index.ts",
  "module": "dist/zm-api-js-client.esm.js",
  "umd:main": "dist/zm-api-js-client.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generateSchemaTypes": "rimraf src/schema/generated-schema-types.ts && graphql-codegen",
    "lint": "tslint --project .",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "BABEL_ENV=test mocha --require ./babelRegisterConfig.js test/**/*.js",
    "test:watch": "npm run test:unit -- --watch",
    "clean": "rimraf dist",
    "build": "npm-run-all clean generateSchemaTypes rollup:* -p build:ts make-minify-executable minify:* copySchema",
    "build:ts": "tsc --emitDeclarationOnly --declaration --outDir \"dist\"",
    "build:analyze": "visualize=true npm run build",
    "copySchema": "copyfiles -f src/schema/schema.graphql dist",
    "rollup:cjs": "cross-var rollup -c rollup.config.js --bundleConfigAsCjs -m -f cjs -n $npm_package_amdName",
    "rollup:umd": "cross-var rollup -c rollup.config.js --bundleConfigAsCjs -m -f umd --environment FORMAT:umd",
    "rollup:esm": "cross-var rollup -c rollup.config.js --bundleConfigAsCjs -m -f es --environment FORMAT:es -n $npm_package_amdName",
    "linkedBuild": "npm run rollup:esm -- --watch",
    "minify:cjs": "scripts/minify.sh dist/zm-api-js-client.js dist/zm-api-js-client.min.js dist/zm-api-js-client.js.map",
    "minify:umd": "scripts/minify.sh dist/zm-api-js-client.umd.js dist/zm-api-js-client.umd.min.js dist/zm-api-js-client.umd.js.map",
    "make-minify-executable": "chmod +x scripts/minify.sh",
    "size": "echo \"Gzipped Size: $(gzip-size $npm_package_main | pretty-bytes)\"",
    "prepublishOnly": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "publish:beta": "npm publish --tag=beta",
    "security:audit": "npx audit-ci@^6 --moderate --report-type summary --pass-enoaudit --skip-dev",
    "prepare": "husky || true"
  },
  "keywords": [
    "zimbra",
    "api",
    "graphql"
  ],
  "repository": "Zimbra/zm-api-js-client",
  "engines": {
    "node": ">=18.12.0"
  },
  "license": "BSD-3-Clause",
  "lint-staged": {
    "**/*.ts": [
      "tslint"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.12.6",
    "@graphql-tools/schema": "^10.0.16",
    "dataloader": "^2.2.3",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "rollup-plugin-ts": "^3.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.1.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-graphql": "^1.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.17.16",
    "@types/whatwg-fetch": "^0.0.33",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.5.0",
    "copyfiles": "^2.4.1",
    "cross-var": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "15.2.8",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^5.0.10",
    "rollup": "^3.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-visualizer": "^5.14.0",
    "tslib": "^2.8.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^5.7.3",
    "uglify-js": "^3.19.3"
  }
}
